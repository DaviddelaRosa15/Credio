{
  "InitialRun": false,
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "SqlServer": "%SQLSERVER%",
    "PostgreSQL": "%POSTGRESQL%"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MailSettings": {
    "EmailFrom": "%SMTPEMAIL%",
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": 587,
    "SmtpUser": "%SMTPEMAIL%",
    "SmtpPass": "%SMTPPASS%",
    "DisplayName": "Base"
  },
  "JWTSettings": {
    "Key": "%JWTKEY%",
    "Issuer": "CodeIdentity",
    "Audience": "Base",
    "DurationInMinutes": 5
  },
  "RefreshJWTSettings": {
    "Key": "%REFRESHKEY%",
    "Issuer": "CodeIdentity",
    "Audience": "Base",
    "DurationInMinutes": 10
  },
  "AllowedHosts": "*"
}